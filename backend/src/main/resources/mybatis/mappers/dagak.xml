<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.backend.dagak.model.mapper.DagakMapper">


    <select id="getCalendarByCalendarDate" parameterType="String" resultType="com.ssafy.backend.dagak.model.domain.Calendar">
        select *
        from calendar
        where calendar_date like CONCAT(#{calendarDate}, '%') or
            calendar_date like CONCAT('%|', #{calendarDate}, '%')
    </select>


</mapper>